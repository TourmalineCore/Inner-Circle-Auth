{
  "name": "inner-circle-auth",
  "version": "0.0.1",
  "private": true,
  "homepage": "/auth",
  "scripts": {
    "start": "run-s create-config:dev start-devserver",
    "start:local": "run-s create-config:local start-devserver",
    "start:local:mocked": "run-s create-config:test start-devserver",
    "start-devserver": "PORT=4000 react-scripts start",
    "build": "cross-env DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "test:component": "npx cypress run --component --browser chrome",
    "test:component:snapshot": "run-s create-config:test && npx jest",
    "test": "run-s test:component test:component:snapshot",
    "eject": "react-scripts eject",
    "release": "standard-version",
    "lint": "run-s lint:js lint:ts lint:css",
    "lint:ts": "eslint --no-error-on-unmatched-pattern ./src/**/*.{ts,tsx}",
    "lint:ts:fix": "eslint --no-error-on-unmatched-pattern ./src/**/*.{ts,tsx} --fix",
    "lint:js": "eslint --no-error-on-unmatched-pattern ./src/**/*.{js,jsx}",
    "lint:js:fix": "eslint --no-error-on-unmatched-pattern ./src/**/*.{js,jsx} --fix",
    "lint:css": "npx stylelint ./src/**/*.css",
    "lint:css:fix": "npx stylelint ./src/**/*.{css,scss} --fix",
    "create-config:dev": "node local-config-builder dev",
    "create-config:local": "node local-config-builder local",
    "create-config:test": "node local-config-builder test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@tourmalinecore/react-tc-auth": "^0.2.1-alpha.0",
    "@tourmalinecore/react-tc-modal": "^0.2.9",
    "@tourmalinecore/react-tc-ui-kit": "^0.4.4",
    "@types/react-test-renderer": "^17.0.2",
    "axios": "^1.2.2",
    "cross-env": "^7.0.3",
    "history": "^5.3.0",
    "husky": "^8.0.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.53.0",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@types/history": "^5.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "clsx": "1.2.1",
    "cypress": "^12.3.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "msw": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-scss": "4.0.4",
    "react-router-dom": "^6.5.0",
    "standard-version": "^9.3.2",
    "stylelint": "^14.5.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.1.0",
    "ts-jest": "^29.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "ie 11"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}